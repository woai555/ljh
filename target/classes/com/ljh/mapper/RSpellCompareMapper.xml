<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ljh.mapper.RSpellCompareMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ljh.bean.RSpellCompare">
        <id column="SPL_ID" property="splId" />
        <result column="SPL_CODE" property="splCode" />
        <result column="SPL_SPELL" property="splSpell" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        SPL_ID, SPL_CODE, SPL_SPELL
    </sql>

</mapper>
