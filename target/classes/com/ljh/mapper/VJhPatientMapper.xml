<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ljh.mapper.VJhPatientMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ljh.bean.VJhPatient">
        <result column="CARD_NO" property="cardNo" />
        <result column="NAME" property="name" />
        <result column="SEX_CODE" property="sexCode" />
        <result column="BIRTHDAY" property="birthday" />
        <result column="IDENNO" property="idenno" />
        <result column="NATION_CODE" property="nationCode" />
        <result column="HOME" property="home" />
        <result column="HOME_TEL" property="homeTel" />
        <result column="MARI" property="mari" />
        <result column="HOME_ZIP" property="homeZip" />
        <result column="LINKMAN_NAME" property="linkmanName" />
        <result column="LINKMAN_ADD" property="linkmanAdd" />
        <result column="LINKMAN_TEL" property="linkmanTel" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        CARD_NO, NAME, SEX_CODE, BIRTHDAY, IDENNO, NATION_CODE, HOME, HOME_TEL, MARI, HOME_ZIP, LINKMAN_NAME, LINKMAN_ADD, LINKMAN_TEL
    </sql>

</mapper>
