<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ljh.mapper.JytLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ljh.bean.JytLog">
        <result column="FUNCTION_NAME" property="functionName" />
        <result column="OPER_DATE" property="operDate" />
        <result column="IN_PARM" property="inParm" />
        <result column="OUT_PARM" property="outParm" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        FUNCTION_NAME, OPER_DATE, IN_PARM, OUT_PARM
    </sql>

</mapper>
