<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ljh.mapper.OrdLocationMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ljh.bean.OrdLocation">
        <id column="LOCATION_ID" property="locationId" />
        <result column="LOCATION_NAME" property="locationName" />
        <result column="DEPT_CODE" property="deptCode" />
        <result column="ORDERID" property="orderid" />
        <result column="SHOW_TEXT" property="showText" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        LOCATION_ID, LOCATION_NAME, DEPT_CODE, ORDERID, SHOW_TEXT
    </sql>

</mapper>
