<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ljh.mapper.UsageWayMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ljh.bean.UsageWay">
        <id column="USE_WAY_CODE" property="useWayCode" />
        <result column="USE_WAY_NAME" property="useWayName" />
        <result column="ENGLISH_AB" property="englishAb" />
        <result column="OPER_DATE" property="operDate" />
