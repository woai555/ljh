<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ljh.mapper.StatTimeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ljh.bean.StatTime">
        <id column="DEPT_CODE" property="deptCode" />
        <result column="REPORT_ID" property="reportId" />
        <result column="REPORT_NAME" property="reportName" />
        <result column="STAT_DATA_FLAG" property="statDataFlag" />
        <result column="STAT_DATETIME" property="statDatetime" />
        <result column="INTERVAL_NUM" property="intervalNum" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        DEPT_CODE, REPORT_ID, REPORT_NAME, STAT_DATA_FLAG, STAT_DATETIME, INTERVAL_NUM
    </sql>

</mapper>
