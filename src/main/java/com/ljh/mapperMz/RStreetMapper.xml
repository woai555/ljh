<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ljh.mapperMz.RStreetMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ljh.bean.RStreet">
        <id column="STREET_CODE" property="streetCode" />
        <result column="STREET_NAME" property="streetName" />
        <result column="SPELL_CODE" property="spellCode" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        STREET_CODE, STREET_NAME, SPELL_CODE
    </sql>

</mapper>
