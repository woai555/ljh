<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ljh.mapperMz.OpSwitchMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ljh.bean.OpSwitch">
        <id column="SWITCH_NO" property="switchNo" />
        <result column="SWITCH_NAME" property="switchName" />
        <result column="SWITCH_VALUE" property="switchValue" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        SWITCH_NO, SWITCH_NAME, SWITCH_VALUE
    </sql>

</mapper>
