<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ljh.mapperMz.UsageCodeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ljh.bean.UsageCode">
        <id column="USAGE_CODE" property="usageCode" />
        <result column="USE_NAME" property="useName" />
        <result column="ENGLISH_AB" property="englishAb" />
        <result column="OPER_DATE" property="operDate" />
        <result column="OPER_CODE" property="operCode" />
        <result column="MR_CODE" property="mrCode" />
        <result column="MR_NAME" property="mrName" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        USAGE_CODE, USE_NAME, ENGLISH_AB, OPER_DATE, OPER_CODE, MR_CODE, MR_NAME
    </sql>

</mapper>
