        <result column="ORDERING_DOC" property="orderingDoc" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        PATIENT_ID, VISIT_ID, VISIT_DATE, PRESCIPTION_NO, DIAGNOSIS, ORDERING_DEPT, ORDERING_DOC
