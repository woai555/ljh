<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ljh.mapper.MInstitutionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ljh.bean.MInstitution">
        <id column="INST_ID" property="instId" />
        <result column="HD_FLAG" property="hdFlag" />
        <result column="NAME" property="name" />
        <result column="HOST_LEVEL" property="hostLevel" />
        <result column="AREA_ID" property="areaId" />
        <result column="ACTION_CODE" property="actionCode" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        INST_ID, HD_FLAG, NAME, HOST_LEVEL, AREA_ID, ACTION_CODE
    </sql>

</mapper>
