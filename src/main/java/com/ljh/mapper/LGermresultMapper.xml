        <result column="ISGERM" property="isgerm" />
        <result column="RESULT" property="result" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        MACHINECODE, CHECKORDER, CHECKDATE, SEQ_NO, GROUPCODE, ISGERM, RESULT
