<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ljh.mapper.LGermresultMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ljh.bean.LGermresult">
        <id column="CHECKORDER" property="checkorder" />
        <result column="MACHINECODE" property="machinecode" />
        <result column="CHECKDATE" property="checkdate" />
        <result column="SEQ_NO" property="seqNo" />
