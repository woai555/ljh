<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ljh.mapper.RBranchMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ljh.bean.RBranch">
        <id column="BRANCH_CODE" property="branchCode" />
        <result column="BRANCH_PY" property="branchPy" />
        <result column="BRANCH_NAME" property="branchName" />
        <result column="OPER_DATE" property="operDate" />
        <result column="OPER_CODE" property="operCode" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        BRANCH_CODE, BRANCH_PY, BRANCH_NAME, OPER_DATE, OPER_CODE
    </sql>

</mapper>
